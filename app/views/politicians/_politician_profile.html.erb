

<div class="col-md-3 col-sm-6 hero-feature">
  <div class="thumbnail">
  <% image_url = "/assets/#{politician.image_name}" %>
      <img src=<%="#{image_url}"%> alt="">
      <div class="caption">
          <h3><%= politician.full_name  %></h3>
          <h4><%= politician.house_or_senate %></h4>
          <p>Age: <%= politician.age %></p>
          <p>Prior Experience: </p>
          <p>Education:</p>
          <p>Email: </p>
          <p>Facebook: <%= politician.facebook %></p>
          <p>Youtube: <%= politician.youtube %></p>
          <p>Twitter: <%= politician.twitter %></p>
      </div>
  </div>
</div>
