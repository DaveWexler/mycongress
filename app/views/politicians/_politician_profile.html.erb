

<div class="panel panel-default col-xs-3 hero-feature whitebox shadow">
  <div class="thumbnail">
  <% image_url = "/assets/#{politician.image_name}" %>
      <img src=<%="#{image_url}"%> alt=""></div>
      <div class="caption">
          <h3><%= politician.full_name  %></h3>
          <h4><%= politician.house_or_senate %></h4>
          <p>Age: <%= politician.age %></p>
          <p>Prior Experience: </p>
          <p>Education:</p>
          <p>Email: </p>
          <p>Facebook: <%= politician.facebook %></p>
          <p>Youtube: <%= politician.youtube %></p>
          <div><a class="btn btn-block btn-social btn-twitter">
  <span class="fa fa-twitter"></span>
</a></div>
          <p>Twitter: <%= politician.twitter %></p>
      </div>
  
</div>
